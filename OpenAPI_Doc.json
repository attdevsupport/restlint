          }
        }
      }
    },
    "/phoenix/encrypt": {
      "post": {
        "tags": [
          "PHOENIX"
        ],
        "summary": "SFDC invoke this API with userId, The API Encrypy userid by using cryptor jar. return the generated token to SFDC. SFDC invoke with this token to PHOENIX",
        "parameters": [
          {
            "name": "request body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/phoenixEncryptInput"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Token for Phoenix",
            "schema": {
              "$ref": "#/definitions/phoenixEncryptResponse"
            }
          },
          "400": {
            "description": "Bad request",
            "schema": {
              "$ref": "#/definitions/phoenixEncryptErrResponse"
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "$ref": "#/definitions/phoenixEncryptErrResponse"
            }
          }
        }
      }
    }
  },
