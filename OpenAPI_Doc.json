{
  "swagger": "2.0",
  "info": {
    "description": "The APIs described in this swagger document are part of Alliance portal. In order to execute some tasks from SFDC application to AT&T, CSP application (Common Security Platform) that located inside the AT&T intranet, FlowDesigner is part of APIONE that aims to allow access to services inside AT&T such as CSP, ROME and SMART",
    "version": "1.0.11",
    "title": "Alliance Portal APIONE APIs",
    "contact": {
      "email": "dj966m@int.att.com"
    }
  },
  "tags": [
    {
      "name": "CSP",
      "description": "The following API`s that preforms action in CSP application"
    },
    {
      "name": "ROME",
      "description": "ROME APIs"
    },
    {
      "name": "SMART",
      "description": "SMART APIs"
    },
    {
      "name": "PHOENIX",
      "description": "PHOENIX APIs"
    }
  ],
  "paths": {
    "/createEntity": {
      "post": {
        "tags": [
          "CSP"
        ],
        "summary": "request that creates a userid, on demand, to ensure that the userid is available. On success, a suspended userid with dummy password is created (ac- countValid=NO). Returns an error if userid is already present. VERY IMORTANT: the  request body describes 2 possibles models 'user' and 'privAdmin' the request can  contains only one of the models descibed",
        "operationId": "create",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "request body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/cspInput"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "success message user/privAdmin not exist and reserved or user created and updated",
            "schema": {
              "$ref": "#/definitions/cspOutput"
            }
          },
          "500": {
            "description": "user/privAdmin exist with updated with details",
            "schema": {
              "$ref": "#/definitions/cspErrorRes"
            }
          }
        }
      }
    },
    "/updateEntity": {
      "post": {
        "tags": [
          "CSP"
        ],
        "operationId": "update",
        "summary": "request that updates CSP information about a particular user",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "request body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/cspInput"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "success message user/priveAdmin updated",
            "schema": {
              "$ref": "#/definitions/cspOutput"
            }
          },
          "500": {
            "description": "error message",
            "schema": {
              "$ref": "#/definitions/cspErrorRes"
            }
          }
        }
      }
    },
    "/rome/company": {
      "post": {
        "tags": [
          "ROME"
        ],
        "parameters": [
          {
            "name": "request body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/companyModel_ROME"
            }
